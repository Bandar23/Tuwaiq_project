  <body>

    <form method="POST" action="/pages/newAutomotiveParts"  enctype="multipart/form-data">
     <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label" dir="rtl">name part</label>
                <input type="text" class="form-control" id="exampleInputEmail1" name="name"
                    aria-describedby="emailHelp" value="">
            </div>

            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" dir="rtl">part type</label>
                <input type="text" class="form-control" name="type" id="exampleInputPassword1" value="">
            </div>

            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" dir="rtl">price</label>
                <input type="number" class="form-control" name="price" id="exampleInputPassword1"
                    value="">
            </div>

             <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" dir="rtl">Date</label>
                <input type="date" class="form-control" name="createAt" id="exampleInputPassword1"
                    value="">
            </div>

              <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label" dir="rtl">part inage</label>
                <input type="file" class="form-control" name="image" accept="image/jpeg,image/jpg" id="exampleInputPassword1"
                    value="">
            </div><br/>
             <div class="mb-3">
                <select class="form-select" name="car" aria-label="Default select example">
                     <option selected>Select Car</option>
                     {{#each items}}
                               <option name="car" value="{{this._id}}">{{this.name}}</option>
                     {{/each}}
                   
                  </select><br/>
        
            <button type="submit" class="btn btn-primary">إضافة</button>
        </form>


